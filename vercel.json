{
  "version":  1,
  "builds" : [
    {
      "src": "client/vite.config.js",
      "use": "@vercel/static-build"
    },
    {
      "src": "server/server.js",
      "dest": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src":"/(.*)",
      "dest":"server/server.js"
    },
    {
      "src":"/(.*)",
      "dest":"/client/$1"
    }
  ]  
}